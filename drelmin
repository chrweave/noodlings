#!/bin/bash
cd /home/cweaver/noodlings

#recompile violently
git fetch origin master
git reset --hard origin/master
gcc -O3 -o ../caw/gramw gramw.c >drelmout 2>&1
../caw/gramw  >>drelmout
head ../caw/eel >>drelmout
#bash ../caw/wurdfold
echo drok$(date +%Y%m%d%H%M) > ../caw/blep
git stage drelmout
git commit -F ../caw/blep drelmout
git push
