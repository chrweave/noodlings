#!/bin/bash
cd /home/cweaver/noodlings


git fetch origin
#git stash
git merge '@{u}'
#git stash pop
#gcc -O3 -o ../caw/lch lch.c
#../caw/rg64 409600 lorque > ../caw/aak
dd if=/dev/urandom bs=256 count=3 |base64 -w 64 > drelmout
echo drok$(date +%Y%m%d%H%M) > ../caw/blep
git stage drelmout
git commit -F ../caw/blep drelmout
git push
